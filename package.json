{
	"name": "vuer",
	"version": "2.2.1",
	"scripts": {
		"start": "npm run dev",
		"dev": "vite",
		"build": "vite build && npm run service-worker",
		"test": "jest",
		"test:watch": "jest --watchAll",
		"test:e2e": "concurrently --success \"first\" -k \"npm:start\" \"cypress run --headed\"",
		"service-worker": "npx rollup -c \"rollup.config.js\"",
		"check": "npm run test && npm run test:e2e"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"vue": "^3.0.0-rc.10"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@testing-library/vue": "^5.0.4",
		"@types/jest": "^26.0.9",
		"@vue/compiler-sfc": "^3.0.0-rc.10",
		"concurrently": "^5.3.0",
		"cypress": "^5.0.0",
		"jest": "^26.4.0",
		"rollup-plugin-terser": "^7.0.1",
		"rollup-plugin-workbox": "^5.2.1",
		"sass": "^1.26.10",
		"ts-jest": "^26.2.0",
		"typescript": "^3.9.7",
		"vite": "^1.0.0-rc.4",
		"workbox-cli": "^5.1.3",
		"workbox-precaching": "^5.1.3"
	}
}
